{
  "name": "mapstronaut",
  "version": "1.0.0",
  "description": "A full-featured JavaScript object mapper",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "format": "prettier --write src/ docs/ README.md",
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "mocha 'src/test/**/*.test.ts'",
    "coverage": "c8 mocha",
    "prebenchmark": "npm run build",
    "benchmark": "node dist/benchmark.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jprevo/mapstronaut.git"
  },
  "keywords": [
    "object-mapper",
    "mapping",
    "automapping",
    "object"
  ],
  "author": "Jonathan Prevost",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jprevo/mapstronaut/issues"
  },
  "homepage": "https://github.com/jprevo/mapstronaut#readme",
  "devDependencies": {
    "@types/jsonpath-plus": "^5.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.1.0",
    "c8": "^10.1.3",
    "mocha": "^11.7.1",
    "prettier": "3.6.2",
    "tinybench": "^4.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "jsonpath-plus": "^10.3.0"
  }
}
